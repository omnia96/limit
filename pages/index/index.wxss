.container{
    width: 100vw;
    display: flex;
    /* color: #fff; */
    /* background-color:grey; */
    flex-direction: column;
    align-items: center;
    z-index: -2;
}
.header {
	width: 100vw;
    height: 60vw;
    background: #00a0e9;
    position: fixed;
    z-index: -1;
    border-radius: 0vw 0vw 7vw 7vw;
    box-shadow: 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23);
}
.location {
	width: 15vw;
    height: 5vw;
    margin-top: 5vw;
    font-size: 5vw;
    margin-bottom: 5vw;
    display: flex;
    align-items: center;
    justify-content: center;
}
.car-card {
	width: 90vw;
	height: 36vw;
	background-image: linear-gradient(to right, #4facfe 0%, #00f2fe 100%);
    border-radius: 1vw;
    margin-bottom: 2vw;
    box-shadow: 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23);
    display: flex;
    flex-direction: column;
    align-items: center;
}
.car-card :hover {
    box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);
}
.car-card-title{
    font-size: 16px;
    font-weight: bolder;
    color: #eeeeee;
    width: 80vw;
    height: 12vw;
    display: flex;
    align-items: flex-end;
}
.car-card-content{
    font-size: 20px;
    font-weight: bolder;
    color: #ffffff;
    width: 80vw;
    height: 12vw;
    display: flex;
    align-items: center;
    justify-content: center;
    animation: car-card-content 1s;
}
.car-card-message{
    font-size: 14px;
    color: #dddddd;
    width: 80vw;
    height: 12vw;
    display: flex;
    align-items: flex-start;
    justify-content: center;
    animation: car-card-message 1s;
}
.tail-number-bar {
	width: 90vw;
	height: 34vw;
	background-color: #ffffff;
    border-radius: 1vw;
    margin-bottom: 2vw;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    box-shadow: 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23);
}
.tail-number-bar-tab{
    width: 90vw;
    height: 10vw;
}
.tail-number-bar-content{
    margin-top: 2vw;
    display: flex;
}
.week {
	width: 10vw;
	height: 20vw;
    margin-left: 1vw;
    margin-right: 1vw;
}
.week-num-top{
    width: 10vw;
    height: 7vw;
    background-color: rgba(0, 50, 100, 0.1);
    border-radius: 1vw 1vw 0vw 0vw;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 3.5vw;
}
.week-num-bot{
    width: 10vw;
    height: 7vw;
    background-color: rgba(0, 50, 100, 0.1);
    border-radius: 0vw 0vw 1vw 1vw;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 3.5vw;
}
.week-num-con{
    width: 10vw;
    height: 6vw;
    font-size: 14px;
    color: #999999;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 3.5vw;
}
.content {
	width: 80vw;
	background-color: #ffffff;
    border-radius: 1vw;
    margin-bottom: 2vw;
    box-shadow: 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23);
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 5vw;
}

.limit-line-title{
    width: 78vw;
    height: 10vw;
    border-left: 1vw solid #4facfe;
    display: flex;
    align-items: center;
    padding-left: 2vw;
    margin-bottom: 2vw;
}
.limit-line-title-start{
    width: 28vw;
    font-size: 16px;
    display: flex;
    align-items: center;
    height: 10vw;
}
.limit-line-title-end{
    width: 50vw;
    font-size: 14px;
    display: flex;
    align-items: center;
    height: 10vw;
}
.limit-line-content{
    display: flex;
    width: 78vw;
    padding-left: 2vw;
    border-top: 1px solid #aaa;
}
.limit-line-content-start{
    width: 28vw;
    font-size: 16px;
    display: flex;
}
.limit-line-content-end{
    width: 50vw;
    font-size: 14px;
    display: flex;
    flex-direction: column;
}
.limit-line-content-end-item{
    width: 49vw;
    height: 10vw;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-left: 2px solid green;
    margin-top: 1vw;
    padding-left: 1vw;
}
.prompt{
    font-size: 14px;
}
@keyframes car-card-content{
    from{
        font-size: 0px;
    }
    to{
        font-size: 20px;
    }
}
@keyframes car-card-message{
    from{
        font-size: 0px;
    }
    to{
        font-size: 14px;
    }
}